{
    "version": 1,
    "formatters": {
        "default": {
            "format": "[%(asctime)s] %(levelname)s in %(module)s: %(message)s"
        }
    },
    "handlers": {
        "file": {
            "class": "logging.FileHandler",
            "formatter": "default",
            "filename": "data/logs/etl.log"
        }
    },
    "root": {
        "level": "INFO",
        "handlers": [
            "file"
        ]
    }
}